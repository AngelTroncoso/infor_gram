<div id="<%= dom_id article %>">
  <p>
    <strong>Title:</strong>
    <%= article.title %>
  </p>

  <p>
    <strong>Content:</strong>
    <%= article.content %>
  </p>

  <p>
    <strong>Location:</strong>
    <%= article.location_id %>
  </p>

  <p>
    <strong>User:</strong>
    <%= article.user_id %>
  </p>

  <p>
    <strong>Available:</strong>
    <%= article.available %>
  </p>

</div>
